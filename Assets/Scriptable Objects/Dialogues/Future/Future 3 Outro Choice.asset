  dialogueChoices:
  - actor: {fileID: 11400000, guid: be784cc64426a7248ba7d795b1148aed, type: 2}
    dialogueOption: (<color="green">Reaper</color>, what is this place?)
    correspondingDialogue: 0
    loopBackToChoice: 1
  - actor: {fileID: 11400000, guid: be784cc64426a7248ba7d795b1148aed, type: 2}
    dialogueOption: (The Heralds and I want to know more about <alpha=#88>Death</color>.)
