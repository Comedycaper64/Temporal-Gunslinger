  dialogueChoices:
  - actor: {fileID: 11400000, guid: 3084d91d8af33cc44be3972a1e63d21c, type: 2}
    dialogueOption: "(If these guards are under <color=\"red\">War</color>\u2019s
      command, wouldn\u2019t it be easier to travel back before he began influencing
