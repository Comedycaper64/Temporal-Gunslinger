      - {fileID: 8300000, guid: ef73d1d5b239e8f4c91a486b5e067bff, type: 3}
    - actor: {fileID: 11400000, guid: 27354eff76e67d941b134882bd3e875d, type: 2}
      actorNo: 0
      dialogue:
      - "So that\u2019s why there\u2019s a charred husk wanderin\u2019 about shootin\u2019
        people for ya? Give me strength, how embarrassin\u2019 is it to need someone
        else t-"
      animations: []
      animationCrossFadeTime: []
      animationTime: []
      cameraModes: 
