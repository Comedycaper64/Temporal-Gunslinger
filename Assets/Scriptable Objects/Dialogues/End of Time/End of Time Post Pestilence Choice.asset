      animations: []
      animationCrossFadeTime: []
      animationTime: []
      cameraModes: 
      disableCameraOnEnd: 0
      voiceClip: []
    - actor: {fileID: 11400000, guid: be632462839849546842682b09217efb, type: 2}
      actorNo: 0
      dialogue:
      - "<size=125%>Hold your tongue. <size=100%>The <color=\"green\">Revenant</color>\u2019s
